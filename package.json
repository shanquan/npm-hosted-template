{
    "name": "admin",
    "version": "0.0.4",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "release": "commit-and-tag-version",
        "serve:v18": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
        "build:v18l": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "core-js": "^3.29.1",
        "crypto-js": "^4.0.0",
        "echarts": "^5.1.1",
        "el-input-ex": "^2.15.14",
        "element-ui": "^2.15.13",
        "jsencrypt": "^3.3.1",
        "user-sys": "^1.1.4",
        "vconsole": "^3.9.1",
        "vue": "^2.7.0",
        "vue-fullscreen": "^2.5.2",
        "vue-i18n": "^8.24.4",
        "vue-router": "^3.5.1",
        "vuex": "^3.6.2",
        "xlsx": "^0.16.9"
    },
    "devDependencies": {
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@vue/cli-plugin-babel": "~4.5.18",
        "@vue/cli-plugin-eslint": "~4.5.18",
        "@vue/cli-service": "~4.5.18",
        "babel-eslint": "^10.1.0",
        "commit-and-tag-version": "^12.4.1",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2"
    },
    "overrides": {
        "cheerio": "1.0.0-rc.12"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "no-mixed-spaces-and-tabs": 0,
            "no-constant-condition": "off",
            "no-unused-vars": "off",
            "no-redeclare": "off",
            "no-extra-semi": "off",
            "no-debugger": "warn"
        }
    },
    "browserslist": [
        "> 10%",
        "last 5 versions",
        "not dead"
    ],
    "engines": {
        "node": "<18"
    }
}
